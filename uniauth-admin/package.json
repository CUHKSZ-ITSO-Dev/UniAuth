{
  "name": "uniauth-umi",
  "version": "6.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "repository": "git@github.com:uniauth/uniauth-umi.git",
  "scripts": {
    "biome:lint": "pnpm exec biome lint",
    "biome:staged": "pnpm exec biome check --write",
    "build": "max build",
    "dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev PORT=5173 max dev",
    "postinstall": "max setup",
    "lint": "pnpm run biome:lint && pnpm run tsc",
    "lint-staged": "lint-staged",
    "openapi": "orval --config orval.config.js",
    "tsc": "tsc --noEmit"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@ant-design/charts": "^2.6.2",
    "@ant-design/icons": "^6.1.0",
    "@ant-design/pro-components": "^2.7.19",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@monaco-editor/react": "^4.7.0",
    "antd": "^5.25.4",
    "antd-style": "^3.7.0",
    "classnames": "^2.5.1",
    "cronstrue": "^3.3.0",
    "dayjs": "^1.11.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-json-view": "^1.21.3",
    "swagger-ui-dist": "^5.28.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^3.3.0",
    "@biomejs/biome": "^2.0.6",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.10",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "^5.3.3",
    "@umijs/max": "^4.3.24",
    "cross-env": "^7.0.3",
    "express": "^4.21.1",
    "gh-pages": "^6.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "orval": "^7.14.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "umi-presets-pro": "^2.0.3",
    "umi-serve": "^1.9.11"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
